<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="besmart.elderlycare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".screen.profile.ProfileActivity"></activity>
        <activity android:name=".screen.chat.ChatActivity" />
        <activity android:name=".screen.notification.NotificationActivity" />
        <activity android:name=".screen.news.NewsActivity" />
        <activity android:name=".screen.knowledge.KnowledgeActivity" />
        <activity android:name=".screen.flie.FlieActivity" />
        <activity android:name=".screen.gps.GpsActivity" />
        <activity android:name=".screen.elderlyprofile.ElderlyProfileActivity" />
        <activity android:name=".screen.calendar.CalendarActivity" />
        <activity android:name=".screen.elderly.ElderlyActivity" />
        <activity android:name=".screen.login.LoginActivity" />
        <activity android:name=".screen.register.RegisterActivity" />
        <activity android:name=".screen.selectusertype.SelectUserTypeActivity" />
        <activity android:name=".screen.welcome.WelcomeActivity" />
        <activity android:name=".screen.splach.SplachScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".screen.main.MainActivity" />
    </application>

</manifest>